   ],
   "source": [
    "for lag in range(1, 5):\n",
    "    data[f'lag_{lag}'] = data['Темп Базовой Инфляции'].shift(lag)\n",
    "\n",
    "#создание переменных для спецификации 1\n",
    "data['t+4'] = data['Темп Базовой Инфляции'].shift(-4)\n",
    "data['t+4-t'] = data['t+4'] - data['Темп Базовой Инфляции']\n",
    "data['t-t-1'] = data['Темп Базовой Инфляции'] - data['lag_1']\n",
    "data['t-1-t-2'] = data['lag_1'] - data['lag_2']\n",
    "data['t-2-t-3'] = data['lag_2'] - data['lag_3']\n",
    "data['t-3-t-4'] = data['lag_3'] - data['lag_4']\n",
    "\n",
    "# Удаление строк с пропущенными значениями\n",
    "data_spec1 = data.dropna(subset=['t+4-t', 't-t-1', 't-1-t-2', 't-2-t-3', 't-3-t-4'])\n",
    "\n",
    "# МНК для спецификации 1\n",
    "X1 = data_spec1[['t-t-1', 't-1-t-2', 't-2-t-3', 't-3-t-4']]\n",
    "X1 = sm.add_constant(X1)  # Добавление константы\n",
    "y1 = data_spec1['t+4-t']\n",
    "model1 = sm.OLS(y1, X1).fit()\n",
    "\n",
    "print(\"Результаты для Спецификации 1:\")\n",
    "model1.summary()"
   ]
  },

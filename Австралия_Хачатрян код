   "source": [
    "# Добавляем лаговую переменную x3_(t-1) (показатель экономической активности: Composite Leading Indicator)\n",
    "data['x3_t-1'] = data['Composite Leading Indicator (CLI)'].shift(1)\n",
    "\n",
    "# Удаление строк с пропущенными значениями\n",
    "data_spec7 = data.dropna(subset=['t+4-t', 't-t-1', 't-1-t-2', 't-2-t-3', 't-3-t-4', 'x3_t-1'])\n",
    "\n",
    "# МНК для спецификации 7\n",
    "X7 = data_spec7[['t-t-1', 't-1-t-2', 't-2-t-3', 't-3-t-4', 'x3_t-1']]\n",
    "X7 = sm.add_constant(X7)  # Добавление константы\n",
    "y7 = data_spec7['t+4-t']\n",
    "model7 = sm.OLS(y7, X7).fit()\n",
    "\n",
    "# Вывод результатов\n",
    "print(\"\\nРезультаты для Спецификации 7:\")\n",
    "model7.summary()"
   ]
  },

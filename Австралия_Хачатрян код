    "# так как все переменные, кроме переменной экономической активности, были созданы\n",
    "# Добавляем лаговую переменную x1_(t-1) (разрыв выпуска реального ВВП)\n",
    "data['x1_t-1'] = data['Разрыв выпуска'].shift(1)\n",
    "\n",
    "# Удаление строк с пропущенными значениями\n",
    "data_spec5 = data.dropna(subset=['t+4-t', 't-t-1', 't-1-t-2', 't-2-t-3', 't-3-t-4', 'x1_t-1'])\n",
    "\n",
    "# МНК для спецификации 5\n",
    "X5 = data_spec5[['t-t-1', 't-1-t-2', 't-2-t-3', 't-3-t-4', 'x1_t-1']]\n",
    "X5 = sm.add_constant(X5)  # Добавление константы\n",
    "y5 = data_spec5['t+4-t']\n",
    "model5 = sm.OLS(y5, X5).fit()\n",
    "\n",
    "# Вывод результатов\n",
    "print(\"\\nРезультаты для Спецификации 5:\")\n",
    "model5.summary()"

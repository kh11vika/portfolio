   "source": [
    "data = pd.read_csv('/Users/kh11vika/Downloads/Хачатрян_Кривенко_Нурдинова - Data.csv') \n",
    "#заменяем запятую на точку в числовых столбцах и преобразуем все числовые столбцы в формат float\n",
    "data = data.replace({',': '.'}, regex=True)\n",
    "data = data.apply(pd.to_numeric, errors='ignore')\n",
    "\n",
    "# Делаем декомпозицию и исключаем сезонность для всех столбцов, кроме темпа роста ВВП, темпа роста базовой инфляции и даты\n",
    "exclude_columns = ['Дата','Темп роста ВВП', 'Темп Базовой Инфляции']\n",
    "\n",
    "for column in data.columns:\n",
    "    if column not in exclude_columns:\n",
    "        # Декомпозиция временного ряда\n",
    "        decomposition = sm.tsa.seasonal_decompose(data[column], model='additive', period=4)  # period=4, т.к. данные квартальные\n",
    "        trend_seasonal_residual = decomposition.trend + decomposition.resid  #исключаем сезонность и тренд\n",
    "        data[column] = trend_seasonal_residual\n",
    "\n",
    "# Округление всех числовых значений до 3 знаков после запятой\n",
    "data = data.round(3)\n",
    "\n",
    "# Проверка результатов\n",
    "data.head()"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "acab55c2-dee6-46d2-a331-894fbd91b8a6",
   "metadata": {

    "# Добавляем лаговую переменную x2_(t-1) (аннуализированный квартальный темп прироста реального ВВП)\n",
    "data['x2_t-1'] = data['Темп прироста ВВП'].shift(1)\n",
    "\n",
    "# Удаление строк с пропущенными значениями\n",
    "data_spec6 = data.dropna(subset=['t+4-t', 't-t-1', 't-1-t-2', 't-2-t-3', 't-3-t-4', 'x2_t-1'])\n",
    "\n",
    "# МНК для спецификации 6\n",
    "X6 = data_spec6[['t-t-1', 't-1-t-2', 't-2-t-3', 't-3-t-4', 'x2_t-1']]\n",
    "X6 = sm.add_constant(X6)  # Добавление константы\n",
    "y6 = data_spec6['t+4-t']\n",
    "model6 = sm.OLS(y6, X6).fit()\n",
    "\n",
    "# Вывод результатов\n",
    "print(\"\\nРезультаты для Спецификации 6:\")\n",

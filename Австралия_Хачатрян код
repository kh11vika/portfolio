    "# Создание переменных для спецификации 2\n",
    "data['mean_last_4_quarters'] = data[['Темп Базовой Инфляции', 'lag_1', 'lag_2', 'lag_3']].mean(axis=1)\n",
    "\n",
    "# Удаление строк с пропущенными значениями\n",
    "data_spec2 = data.dropna(subset=['t+4', 'mean_last_4_quarters'])\n",
    "\n",
    "# МНК для спецификации 2\n",
    "X2 = sm.add_constant(data_spec2['mean_last_4_quarters'])\n",
    "y2 = data_spec2['t+4']\n",
    "model2 = sm.OLS(y2, X2).fit()\n",
    "\n",
    "print(\"\\nРезультаты для Спецификации 2:\")\n",
    "model2.summary()"

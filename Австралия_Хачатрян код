   "source": [
    "# Создание переменной для спецификации 4\n",
    "data['lag_exp_ifl_3'] = data['Инфляционные ожидания'].shift(3)\n",
    "data['year_exp_infl_growth'] = data['Инфляционные ожидания'] - data['lag_exp_ifl_3']\n",
    "\n",
    "# Удаление строк с пропущенными значениями\n",
    "data_spec4 = data.dropna(subset=['t+4', 'year_exp_infl_growth'])\n",
    "\n",
    "# МНК для спецификации 4\n",
    "X4 = sm.add_constant(data_spec4['year_exp_infl_growth'])\n",
    "y4 = data_spec4['t+4']\n",
    "model4 = sm.OLS(y4, X4).fit()\n",
    "\n",
    "print(\"\\nРезультаты для Спецификации 4:\")\n",
    "model4.summary()"
   ]
